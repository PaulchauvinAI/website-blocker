{
    "manifest_version": 3,
    "name": "Website Blocker",
    "version": "1.0",
    "description": "Block specific websites during certain timeframes and redirect to another URL.",
    "permissions": [
      "storage",
      "webNavigation",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Open Blocker Settings"
    },
    "options_page": "options.html"
  }
  